	<property name="com.jaspersoft.studio.data.defaultdataadapter" value="P2B - ISFAST SIT"/>
	<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
	<parameter name="P_APPL_YR" class="java.lang.String">
		<parameterDescription><![CDATA[]]></parameterDescription>
		<defaultValueExpression><![CDATA["2018"]]></defaultValueExpression>
	</parameter>
	<parameter name="P_CUT_OFF_DATE" class="java.lang.String">
		<parameterDescription><![CDATA[]]></parameterDescription>
		<defaultValueExpression><![CDATA["20181031"]]></defaultValueExpression>
	</parameter>
	<queryString language="SQL">
		<![CDATA[-- SDFA_SUMMARY
SELECT ITEM, NLSFT, ENLS, NLSPS, TSFS, SFAS, JFS, APR 
  FROM (
-- STUDENT_CNT (No. of Students)
        SELECT 'STUDENT_CNT' AS ITEM             
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(SC.SCHEME_CODE, 'NLSFT', SC.STUDENT_CNT)), '999999999999'))) NLSFT
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(SC.SCHEME_CODE, 'ENLS', SC.STUDENT_CNT)), '999999999999'))) ENLS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(SC.SCHEME_CODE, 'NLSPS', SC.STUDENT_CNT)), '999999999999'))) NLSPS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(SC.SCHEME_CODE, 'TSFS', SC.STUDENT_CNT)), '999999999999'))) TSFS
             , '-' SFAS, '-' JFS, '-' APR
          FROM (
                -- NLSFT
                (SELECT A.SCHEME_CODE, SUM(A.STUDENT_CNT - NVL(B.STUDENT_CNT, 0)) STUDENT_CNT
                   FROM (SELECT LOAN_STATUS, SCHEME_CODE, COUNT(DISTINCT APPLICANT_HKID) STUDENT_CNT FROM NLS_REP_STAT_LOAN_REP 
                          WHERE SCHEME_CODE = 'NLSFT' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) A
                      , (SELECT LOAN_STATUS, COUNT(DISTINCT HKID) STUDENT_CNT, SCHEME_CODE FROM NLS_REP_STAT_FULLY_SETTLED 
                          WHERE SCHEME_CODE = 'NLSFT' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) B
                  WHERE A.LOAN_STATUS = B.LOAN_STATUS (+)
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- ENLS
                (SELECT A.SCHEME_CODE, SUM(A.STUDENT_CNT - NVL(B.STUDENT_CNT, 0)) STUDENT_CNT
                   FROM (SELECT T.PRCP_STS LOAN_STATUS, T.SCHEME_CODE, SUM(T.NO_RECP) STUDENT_CNT
                           FROM (SELECT PRCP_STS, REPAYM_MODE, SCHEME_CODE, NO_RECP FROM ENLS_REP_STAT_LOAN_REP 
                                  WHERE SCHEME_CODE = 'ENLS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND PRIN_PAID >= 0
                                 UNION ALL
                                 SELECT PRCP_STS, REPAYM_MODE, SCHEME_CODE, NO_RECP FROM ENLS_REP_STAT_LOAN_REP_ADJ) T
                          GROUP BY T.PRCP_STS, T.SCHEME_CODE) A
                      , (SELECT S.LN_PRCS_STS LOAN_STATUS, SUM(S.STUDENT_CNT) STUDENT_CNT 
                           FROM (SELECT EI_CDE, LN_PRCS_STS, REPAYM_MODE, SCHEME_CODE, COUNT(DISTINCT CTIF_ID_NO) STUDENT_CNT FROM ENLS_REP_STAT_FULLY_SETTLED 
                                  WHERE SCHEME_CODE = 'ENLS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY EI_CDE, LN_PRCS_STS, SCHEME_CODE, REPAYM_MODE) S  
                          GROUP BY S.LN_PRCS_STS) B
                  WHERE A.LOAN_STATUS = B.LOAN_STATUS (+) 
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- NLSPS
                (SELECT A.SCHEME_CODE, SUM(A.STUDENT_CNT - NVL(B.STUDENT_CNT, 0)) STUDENT_CNT
                   FROM (SELECT LOAN_STATUS, SCHEME_CODE, COUNT(DISTINCT APPLICANT_HKID) STUDENT_CNT FROM NLS_REP_STAT_LOAN_REP
                          WHERE SCHEME_CODE = 'NLSPS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) A
                      , (SELECT LOAN_STATUS, SCHEME_CODE, COUNT(DISTINCT HKID) STUDENT_CNT FROM NLS_REP_STAT_FULLY_SETTLED
                          WHERE SCHEME_CODE = 'NLSPS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) B
                  WHERE A.LOAN_STATUS = B.LOAN_STATUS (+)
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- TSFS
                (SELECT AL.SCHEME_CODE, SUM(AL.STUDENT_CNT - NVL(SL.STUDENT_CNT, 0)) STUDENT_CNT
                   FROM (SELECT A.LOAN_STATUS, A.SCHEME_CODE, SUM(A.STUDENT_CNT) STUDENT_CNT
                           FROM (SELECT T.EI_CDE, T.LN_PRCS_STS LOAN_STATUS, T.SCHEME_CODE, COUNT(DISTINCT T.CTIF_ID_NO) STUDENT_CNT
                                   FROM (SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE, CTIF_ID_NO
                                           FROM SFS_REP_STAT_LOAN_REP WHERE SCHEME_CODE = 'TSFS'AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND FIN_SCHM_ABBR = 'TSFS'
                                         UNION ALL
                                         SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE, CTIF_ID_NO
                                           FROM SFS_REP_STAT_LOAN_REP_ADJ WHERE SCHEME_CODE = 'TSFS' AND FIN_SCHM_ABBR = 'TSFS') T
                                  GROUP BY T.EI_CDE, T.LN_PRCS_STS, T.SCHEME_CODE) A
                          GROUP BY A.LOAN_STATUS, A.SCHEME_CODE) AL
                      , (SELECT S.LN_PRCS_STS LOAN_STATUS, S.SCHEME_CODE, SUM(S.STUDENT_CNT) STUDENT_CNT
                           FROM (SELECT T.LN_PRCS_STS, T.EI_CDE, T.SCHEME_CODE, COUNT(DISTINCT T.CTIF_ID_NO) STUDENT_CNT
                                   FROM (SELECT LN_PRCS_STS, EI_CDE, SCHEME_CODE, CTIF_ID_NO
                                           FROM SFS_REP_STAT_FULLY_SETTLED WHERE SCHEME_CODE = 'TSFS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND FIN_SCHM_ABBR = 'TSFS'
                                          UNION ALL
                                         SELECT LN_PRCS_STS, EI_CDE, SCHEME_CODE, CTIF_ID_NO
                                           FROM SFS_REP_STAT_FULLY_SETTLED_ADJ WHERE SCHEME_CODE = 'TSFS'AND FIN_SCHM_ABBR = 'TSFS') T
                                  GROUP BY T.LN_PRCS_STS, T.EI_CDE, T.SCHEME_CODE) S
                          GROUP BY S.LN_PRCS_STS, S.SCHEME_CODE) SL
                  WHERE AL.LOAN_STATUS = SL.LOAN_STATUS (+)
                  GROUP BY AL.SCHEME_CODE)
               ) SC
        
        UNION ALL
        
-- LOAN_CNT (No. of Accounts)
        SELECT 'LOAN_CNT' AS ITEM
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(LC.SCHEME_CODE, 'NLSFT', LC.LOAN_CNT)), '999999999999'))) NLSFT
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(LC.SCHEME_CODE, 'ENLS', LC.LOAN_CNT)), '999999999999'))) ENLS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(LC.SCHEME_CODE, 'NLSPS', LC.LOAN_CNT)), '999999999999'))) NLSPS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(LC.SCHEME_CODE, 'TSFS', LC.LOAN_CNT)), '999999999999'))) TSFS
             , '-' SFAS, '-' JFS, '-' APR
          FROM (
                -- NLSFT
                (SELECT A.SCHEME_CODE, SUM(A.LOAN_CNT - NVL(B.LOAN_CNT, 0)) LOAN_CNT
                   FROM (SELECT LOAN_STATUS, SCHEME_CODE, COUNT(LOAN_REF_NUM) LOAN_CNT FROM NLS_REP_STAT_LOAN_REP 
                          WHERE SCHEME_CODE = 'NLSFT' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) A
                      , (SELECT LOAN_STATUS, SCHEME_CODE, COUNT(LOAN_REF_NUM) LOAN_CNT FROM NLS_REP_STAT_FULLY_SETTLED
                          WHERE SCHEME_CODE = 'NLSFT' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) B
                  WHERE A.LOAN_STATUS = B.LOAN_STATUS (+)
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- ENLS
                (SELECT A.SCHEME_CODE, SUM(A.LOAN_CNT - NVL(B.LOAN_CNT, 0)) LOAN_CN
                   FROM (SELECT T.PRCP_STS LOAN_STATUS, T.SCHEME_CODE, SUM(T.NO_ACCT) LOAN_CNT
                           FROM (SELECT PRCP_STS, REPAYM_MODE, SCHEME_CODE, NO_ACCT FROM ENLS_REP_STAT_LOAN_REP
                                  WHERE SCHEME_CODE = 'ENLS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND PRIN_PAID >= 0
                                 UNION ALL
                                 SELECT PRCP_STS, REPAYM_MODE, SCHEME_CODE, NO_ACCT FROM ENLS_REP_STAT_LOAN_REP_ADJ) T
                          GROUP BY T.PRCP_STS, T.SCHEME_CODE) A  
                      , (SELECT S.LN_PRCS_STS LOAN_STATUS, SUM(S.LOAN_CNT) LOAN_CNT
                           FROM (SELECT EI_CDE, LN_PRCS_STS, REPAYM_MODE, SCHEME_CODE, COUNT(LOAN_NO) LOAN_CNT FROM ENLS_REP_STAT_FULLY_SETTLED
                                  WHERE SCHEME_CODE = 'ENLS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY EI_CDE, LN_PRCS_STS, SCHEME_CODE, REPAYM_MODE) S
                          GROUP BY S.LN_PRCS_STS) B
                  WHERE A.LOAN_STATUS = B.LOAN_STATUS (+)     
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- NLSPS        
                (SELECT A.SCHEME_CODE, SUM(A.LOAN_CNT - NVL(B.LOAN_CNT, 0)) LOAN_CNT
                   FROM (SELECT LOAN_STATUS, SCHEME_CODE, COUNT(LOAN_REF_NUM) LOAN_CNT FROM NLS_REP_STAT_LOAN_REP 
                          WHERE SCHEME_CODE = 'NLSPS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) A
                      , (SELECT LOAN_STATUS, SCHEME_CODE, COUNT(LOAN_REF_NUM) LOAN_CNT FROM NLS_REP_STAT_FULLY_SETTLED
                          WHERE SCHEME_CODE = 'NLSPS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) B
                  WHERE A.LOAN_STATUS = B.LOAN_STATUS (+)
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- TSFS
                (SELECT AL.SCHEME_CODE, SUM(AL.LOAN_CNT - NVL(SL.LOAN_CNT, 0)) LOAN_CNT
                   FROM (SELECT A.LOAN_STATUS, A.SCHEME_CODE, SUM(A.LOAN_CNT) LOAN_CNT
                           FROM (SELECT T.EI_CDE, T.LN_PRCS_STS LOAN_STATUS, T.SCHEME_CODE,COUNT(T.LOAN_NO) LOAN_CNT
                                   FROM (SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE, LOAN_NO
                                           FROM SFS_REP_STAT_LOAN_REP WHERE SCHEME_CODE = 'TSFS'AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND FIN_SCHM_ABBR = 'TSFS'
                                         UNION ALL
                                         SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE, LOAN_NO
                                           FROM SFS_REP_STAT_LOAN_REP_ADJ WHERE SCHEME_CODE = 'TSFS' AND FIN_SCHM_ABBR = 'TSFS') T
                                  GROUP BY T.EI_CDE, T.LN_PRCS_STS, T.SCHEME_CODE) A
                          GROUP BY A.LOAN_STATUS, A.SCHEME_CODE) AL
                      , (SELECT S.LN_PRCS_STS LOAN_STATUS, S.SCHEME_CODE, SUM(S.LOAN_CNT) LOAN_CNT
                           FROM (SELECT T.LN_PRCS_STS, T.EI_CDE, T.SCHEME_CODE, COUNT(T.LOAN_NO) LOAN_CNT
                                   FROM (SELECT LN_PRCS_STS, EI_CDE, SCHEME_CODE, LOAN_NO
                                           FROM SFS_REP_STAT_FULLY_SETTLED WHERE SCHEME_CODE = 'TSFS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND FIN_SCHM_ABBR = 'TSFS'
                                          UNION ALL
                                         SELECT LN_PRCS_STS, EI_CDE, SCHEME_CODE, LOAN_NO
                                           FROM SFS_REP_STAT_FULLY_SETTLED_ADJ WHERE SCHEME_CODE = 'TSFS'AND FIN_SCHM_ABBR = 'TSFS') T
                                  GROUP BY T.LN_PRCS_STS, T.EI_CDE, T.SCHEME_CODE) S
                          GROUP BY S.LN_PRCS_STS, S.SCHEME_CODE) SL
                  WHERE AL.LOAN_STATUS = SL.LOAN_STATUS (+)
                  GROUP BY AL.SCHEME_CODE)
               ) LC
               
        UNION ALL
        
-- PRINCIPAL_UNDEMANDED (Undemanded principal ($M))
        SELECT 'PRINCIPAL_UNDEMANDED' AS ITEM
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(PUN.SCHEME_CODE, 'NLSFT', PUN.PRINCIPAL_UNDEMANDED)), '999,999,999,999.000'))) NLSFT
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(PUN.SCHEME_CODE, 'ENLS', PUN.PRINCIPAL_UNDEMANDED)), '999,999,999,999.000'))) ENLS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(PUN.SCHEME_CODE, 'NLSPS', PUN.PRINCIPAL_UNDEMANDED)), '999,999,999,999.000'))) NLSPS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(PUN.SCHEME_CODE, 'TSFS', PUN.PRINCIPAL_UNDEMANDED)), '999,999,999,999.000'))) TSFS
             , '-' SFAS, '-' JFS, '-' APR
          FROM (
                -- NLSFT
                (SELECT A.SCHEME_CODE, ROUND(SUM(A.PRINCIPAL_UNDEMANDED) / 1000000, 3) PRINCIPAL_UNDEMANDED
                   FROM (SELECT LOAN_STATUS, SCHEME_CODE, SUM(UNDEMANDED_PRINCIPAL) PRINCIPAL_UNDEMANDED FROM NLS_REP_STAT_LOAN_REP 
                          WHERE SCHEME_CODE = 'NLSFT' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) A
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- ENLS
                (SELECT A.SCHEME_CODE, ROUND(SUM(A.PRINCIPAL_UNDEMANDED) / 1000000, 3) PRINCIPAL_UNDEMANDED
                   FROM (SELECT T.PRCP_STS LOAN_STATUS, T.SCHEME_CODE, SUM(T.UNDER_PRINC) PRINCIPAL_UNDEMANDED 
                           FROM (SELECT PRCP_STS, REPAYM_MODE, SCHEME_CODE, UNDER_PRINC FROM ENLS_REP_STAT_LOAN_REP 
                                  WHERE SCHEME_CODE = 'ENLS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND PRIN_PAID >= 0
                                 UNION ALL
                                 SELECT PRCP_STS, REPAYM_MODE, SCHEME_CODE, UNDER_PRINC FROM ENLS_REP_STAT_LOAN_REP_ADJ) T
                          GROUP BY T.PRCP_STS, T.SCHEME_CODE) A
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- NLSPS
                (SELECT A.SCHEME_CODE, ROUND(SUM(A.PRINCIPAL_UNDEMANDED) / 1000000, 3) PRINCIPAL_UNDEMANDED
                   FROM (SELECT LOAN_STATUS, SCHEME_CODE, SUM(UNDEMANDED_PRINCIPAL) PRINCIPAL_UNDEMANDED FROM NLS_REP_STAT_LOAN_REP 
                          WHERE SCHEME_CODE = 'NLSPS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) A
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- TSFS
                (SELECT AL.SCHEME_CODE, ROUND(SUM(AL.PRINCIPAL_UNDEMANDED) / 1000000, 3) PRINCIPAL_UNDEMANDED
                   FROM (SELECT A.LOAN_STATUS, A.SCHEME_CODE, SUM(A.PRINCIPAL_UNDEMANDED) PRINCIPAL_UNDEMANDED
                           FROM (SELECT T.EI_CDE, T.LN_PRCS_STS LOAN_STATUS, T.SCHEME_CODE, SUM(T.UNDEMAND_PRCP) PRINCIPAL_UNDEMANDED
                                   FROM (SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE, UNDEMAND_PRCP
                                           FROM SFS_REP_STAT_LOAN_REP WHERE SCHEME_CODE = 'TSFS'AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND FIN_SCHM_ABBR = 'TSFS'
                                         UNION ALL
                                         SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE, UNDEMAND_PRCP
                                           FROM SFS_REP_STAT_LOAN_REP_ADJ WHERE SCHEME_CODE = 'TSFS' AND FIN_SCHM_ABBR = 'TSFS') T
                                  GROUP BY T.EI_CDE, T.LN_PRCS_STS, T.SCHEME_CODE) A
                          GROUP BY A.LOAN_STATUS, A.SCHEME_CODE) AL
                  GROUP BY AL.SCHEME_CODE)
               ) PUN
                
        UNION ALL
        
-- PRINCIPAL_ARREARS (Principal in arrears ($M))
        SELECT 'PRINCIPAL_ARREARS' AS ITEM
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(PAR.SCHEME_CODE, 'NLSFT', PAR.PRINCIPAL_ARREARS)), '999,999,999,999.000'))) NLSFT
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(PAR.SCHEME_CODE, 'ENLS', PAR.PRINCIPAL_ARREARS)), '999,999,999,999.000'))) ENLS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(PAR.SCHEME_CODE, 'NLSPS', PAR.PRINCIPAL_ARREARS)), '999,999,999,999.000'))) NLSPS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(PAR.SCHEME_CODE, 'TSFS', PAR.PRINCIPAL_ARREARS)), '999,999,999,999.000'))) TSFS
             , '-' SFAS, '-' JFS, '-' APR
          FROM (
                -- NLSFT
                (SELECT A.SCHEME_CODE, ROUND(SUM(A.PRINCIPAL_ARREARS / 1000000), 3) PRINCIPAL_ARREARS
                   FROM (SELECT LOAN_STATUS, SCHEME_CODE, SUM(PRINCIPAL_UNPAID) PRINCIPAL_ARREARS FROM NLS_REP_STAT_LOAN_REP 
                          WHERE SCHEME_CODE = 'NLSFT' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) A
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- ENLS
                (SELECT A.SCHEME_CODE, ROUND(SUM(A.PRINCIPAL_ARREARS) / 1000000, 3) PRINCIPAL_ARREARS
                   FROM (SELECT T.PRCP_STS LOAN_STATUS, T.SCHEME_CODE, SUM(T.PRIN_IN_ARREAR) PRINCIPAL_ARREARS
                           FROM (SELECT PRCP_STS, REPAYM_MODE, SCHEME_CODE, PRIN_IN_ARREAR FROM ENLS_REP_STAT_LOAN_REP
                                  WHERE SCHEME_CODE = 'ENLS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND PRIN_PAID >= 0
                                 UNION ALL
                                 SELECT PRCP_STS, REPAYM_MODE, SCHEME_CODE, PRIN_IN_ARREAR FROM ENLS_REP_STAT_LOAN_REP_ADJ) T
                          GROUP BY T.PRCP_STS, T.SCHEME_CODE) A
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- NLSPS
                (SELECT A.SCHEME_CODE, ROUND(SUM(A.PRINCIPAL_ARREARS) / 1000000, 3) PRINCIPAL_ARREARS
                   FROM (SELECT LOAN_STATUS, SCHEME_CODE, SUM(PRINCIPAL_UNPAID) PRINCIPAL_ARREARS FROM NLS_REP_STAT_LOAN_REP 
                          WHERE SCHEME_CODE = 'NLSPS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) A
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- TSFS
                (SELECT AL.SCHEME_CODE, ROUND(SUM(AL.PRINCIPAL_ARREARS) / 1000000, 3) PRINCIPAL_ARREARS
                   FROM (SELECT A.LOAN_STATUS, A.SCHEME_CODE, SUM(A.PRINCIPAL_ARREARS) PRINCIPAL_ARREARS
                           FROM (SELECT T.EI_CDE, T.LN_PRCS_STS LOAN_STATUS, T.SCHEME_CODE, SUM(T.ARREAR_PRCP) PRINCIPAL_ARREARS
                                   FROM (SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE, ARREAR_PRCP
                                           FROM SFS_REP_STAT_LOAN_REP WHERE SCHEME_CODE = 'TSFS'AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND FIN_SCHM_ABBR = 'TSFS'
                                         UNION ALL
                                         SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE, ARREAR_PRCP
                                           FROM SFS_REP_STAT_LOAN_REP_ADJ WHERE SCHEME_CODE = 'TSFS' AND FIN_SCHM_ABBR = 'TSFS') T
                                  GROUP BY T.EI_CDE, T.LN_PRCS_STS, T.SCHEME_CODE) A
                          GROUP BY A.LOAN_STATUS, A.SCHEME_CODE) AL
                  GROUP BY AL.SCHEME_CODE)
               ) PAR
               
        UNION ALL
        
-- REPAYING_LOAN_CNT (Total No. of Repayment Accounts)
        SELECT 'REPAYING_LOAN_CNT' AS ITEM
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(RLC.SCHEME_CODE, 'NLSFT', RLC.REPAYING_LOAN_CNT)), '999999999999'))) NLSFT
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(RLC.SCHEME_CODE, 'ENLS', RLC.REPAYING_LOAN_CNT)), '999999999999'))) ENLS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(RLC.SCHEME_CODE, 'NLSPS', RLC.REPAYING_LOAN_CNT)), '999999999999'))) NLSPS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(RLC.SCHEME_CODE, 'TSFS', RLC.REPAYING_LOAN_CNT)), '999999999999'))) TSFS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(RLC.SCHEME_CODE, 'SFAS', RLC.REPAYING_LOAN_CNT)), '999999999999'))) SFAS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(RLC.SCHEME_CODE, 'JFS', RLC.REPAYING_LOAN_CNT)), '999999999999'))) JFS
             , '-' APR
          FROM (
                -- NLSFT
                (SELECT A.SCHEME_CODE, SUM(A.REPAYING_LOAN_CNT - NVL(B.REPAYING_LOAN_CNT, 0)) REPAYING_LOAN_CNT
                   FROM (SELECT LOAN_STATUS, SCHEME_CODE, SUM(DECODE(LOAN_STATUS, 'R', 1, 0)) REPAYING_LOAN_CNT FROM NLS_REP_STAT_LOAN_REP 
                          WHERE SCHEME_CODE = 'NLSFT' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) A
                      , (SELECT LOAN_STATUS, SCHEME_CODE, SUM(DECODE(LOAN_STATUS, 'R', 1, 0)) REPAYING_LOAN_CNT FROM NLS_REP_STAT_FULLY_SETTLED
                          WHERE SCHEME_CODE = 'NLSFT' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) B
                  WHERE A.LOAN_STATUS = B.LOAN_STATUS (+)
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- ENLS
                (SELECT A.SCHEME_CODE, SUM(A.REPAYING_LOAN_CNT - NVL(B.REPAYING_LOAN_CNT, 0)) REPAYING_LOAN_CNT
                   FROM (SELECT T.PRCP_STS LOAN_STATUS, T.SCHEME_CODE, SUM(DECODE(T.PRCP_STS, 'R', T.NO_ACCT, 0)) REPAYING_LOAN_CNT
                           FROM (SELECT PRCP_STS, REPAYM_MODE, SCHEME_CODE, NO_ACCT FROM ENLS_REP_STAT_LOAN_REP
                                  WHERE SCHEME_CODE = 'ENLS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND PRIN_PAID >= 0
                                 UNION ALL
                                 SELECT PRCP_STS, REPAYM_MODE, SCHEME_CODE, NO_ACCT FROM ENLS_REP_STAT_LOAN_REP_ADJ) T
                          GROUP BY T.PRCP_STS, T.SCHEME_CODE) A
                      , (SELECT S.LN_PRCS_STS LOAN_STATUS, SUM(DECODE(S.LN_PRCS_STS, 'R', S.LOAN_CNT, 0)) REPAYING_LOAN_CNT   
                           FROM (SELECT EI_CDE, LN_PRCS_STS, REPAYM_MODE, SCHEME_CODE, COUNT(LOAN_NO) LOAN_CNT FROM ENLS_REP_STAT_FULLY_SETTLED 
                                  WHERE SCHEME_CODE = 'ENLS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY EI_CDE, LN_PRCS_STS, REPAYM_MODE, SCHEME_CODE) S
                          GROUP BY S.LN_PRCS_STS) B 
                  WHERE A.LOAN_STATUS = B.LOAN_STATUS (+)         
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- NLSPS
                (SELECT A.SCHEME_CODE, SUM(A.REPAYING_LOAN_CNT - NVL(B.REPAYING_LOAN_CNT, 0)) REPAYING_LOAN_CNT
                   FROM (SELECT LOAN_STATUS, SCHEME_CODE, SUM(DECODE(LOAN_STATUS, 'R', 1, 0)) REPAYING_LOAN_CNT FROM NLS_REP_STAT_LOAN_REP 
                          WHERE SCHEME_CODE = 'NLSPS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) A
                      , (SELECT LOAN_STATUS, SCHEME_CODE, SUM(DECODE(LOAN_STATUS, 'R', 1, 0)) REPAYING_LOAN_CNT FROM NLS_REP_STAT_FULLY_SETTLED
                          WHERE SCHEME_CODE = 'NLSPS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) B
                  WHERE A.LOAN_STATUS = B.LOAN_STATUS (+)
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- TSFS
                (SELECT AL.SCHEME_CODE, SUM(AL.REPAYING_LOAN_CNT - NVL(SL.REPAYING_LOAN_CNT, 0)) REPAYING_LOAN_CNT
                   FROM (SELECT A.LOAN_STATUS, A.SCHEME_CODE, SUM(A.REPAYING_LOAN_CNT) REPAYING_LOAN_CNT
                           FROM (SELECT T.EI_CDE, T.LN_PRCS_STS LOAN_STATUS, T.SCHEME_CODE, SUM(DECODE(T.LN_PRCS_STS, 'R', 1, 0)) REPAYING_LOAN_CNT
                                   FROM (SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE
                                           FROM SFS_REP_STAT_LOAN_REP WHERE SCHEME_CODE = 'TSFS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND FIN_SCHM_ABBR = 'TSFS'
                                         UNION ALL
                                         SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE
                                           FROM SFS_REP_STAT_LOAN_REP_ADJ WHERE SCHEME_CODE = 'TSFS' AND FIN_SCHM_ABBR = 'TSFS') T
                                  GROUP BY T.EI_CDE, T.LN_PRCS_STS, T.SCHEME_CODE) A
                          GROUP BY A.LOAN_STATUS, A.SCHEME_CODE) AL
                      , (SELECT S.LN_PRCS_STS LOAN_STATUS, S.SCHEME_CODE, SUM(S.REPAYING_LOAN_CNT) REPAYING_LOAN_CNT
                           FROM (SELECT T.LN_PRCS_STS, T.EI_CDE, T.SCHEME_CODE, SUM(DECODE(T.LN_PRCS_STS, 'R', 1, 0)) REPAYING_LOAN_CNT
                                   FROM (SELECT LN_PRCS_STS, EI_CDE, SCHEME_CODE
                                           FROM SFS_REP_STAT_FULLY_SETTLED WHERE SCHEME_CODE = 'TSFS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND FIN_SCHM_ABBR = 'TSFS'
                                          UNION ALL
                                         SELECT LN_PRCS_STS, EI_CDE, SCHEME_CODE
                                           FROM SFS_REP_STAT_FULLY_SETTLED_ADJ WHERE SCHEME_CODE = 'TSFS'AND FIN_SCHM_ABBR = 'TSFS') T
                                  GROUP BY T.LN_PRCS_STS, T.EI_CDE, T.SCHEME_CODE) S
                          GROUP BY S.LN_PRCS_STS, S.SCHEME_CODE) SL
                  WHERE AL.LOAN_STATUS = SL.LOAN_STATUS (+)
                  GROUP BY AL.SCHEME_CODE)
                UNION ALL                 
                -- SFAS
                (SELECT 'SFAS' AS SCHEME_CODE, SUM(AL.REPAYING_LOAN_CNT - NVL(SL.REPAYING_LOAN_CNT, 0)) REPAYING_LOAN_CNT
                   FROM (SELECT A.LOAN_STATUS, A.SCHEME_CODE, SUM(A.REPAYING_LOAN_CNT) REPAYING_LOAN_CNT
                           FROM (SELECT T.EI_CDE, T.LN_PRCS_STS LOAN_STATUS, T.SCHEME_CODE, SUM(DECODE(T.LN_PRCS_STS, 'R', 1, 0)) REPAYING_LOAN_CNT
                                   FROM (SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE
                                           FROM SFS_REP_STAT_LOAN_REP WHERE SCHEME_CODE = 'TSFS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND FIN_SCHM_ABBR = 'SFAS'
                                         UNION ALL
                                         SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE 
                                           FROM SFS_REP_STAT_LOAN_REP_ADJ WHERE SCHEME_CODE = 'TSFS' AND FIN_SCHM_ABBR = 'SFAS') T
                                  GROUP BY T.EI_CDE, T.LN_PRCS_STS, T.SCHEME_CODE) A
                          GROUP BY A.LOAN_STATUS, A.SCHEME_CODE) AL
                      , (SELECT S.LN_PRCS_STS LOAN_STATUS, S.SCHEME_CODE, SUM(S.REPAYING_LOAN_CNT) REPAYING_LOAN_CNT
                           FROM (SELECT T.LN_PRCS_STS, T.EI_CDE, T.SCHEME_CODE, SUM(DECODE(T.LN_PRCS_STS, 'R', 1, 0)) REPAYING_LOAN_CNT
                                   FROM (SELECT LN_PRCS_STS, EI_CDE, SCHEME_CODE
                                           FROM SFS_REP_STAT_FULLY_SETTLED WHERE SCHEME_CODE = 'TSFS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND FIN_SCHM_ABBR = 'SFAS'
                                         UNION ALL
                                         SELECT LN_PRCS_STS, EI_CDE, SCHEME_CODE
                                           FROM SFS_REP_STAT_FULLY_SETTLED_ADJ WHERE SCHEME_CODE = 'TSFS' AND FIN_SCHM_ABBR = 'SFAS') T
                                  GROUP BY T.LN_PRCS_STS, T.EI_CDE, T.SCHEME_CODE) S
                          GROUP BY S.LN_PRCS_STS, S.SCHEME_CODE) SL
                  WHERE AL.LOAN_STATUS = SL.LOAN_STATUS (+)
                  GROUP BY AL.SCHEME_CODE)
                UNION ALL
                -- JFS
                (SELECT 'JFS' AS SCHEME_CODE, SUM(AL.REPAYING_LOAN_CNT - NVL(SL.REPAYING_LOAN_CNT, 0)) REPAYING_LOAN_CNT 
                   FROM (SELECT A.LOAN_STATUS, A.SCHEME_CODE, SUM(A.REPAYING_LOAN_CNT) REPAYING_LOAN_CNT
                           FROM (SELECT T.EI_CDE, T.LN_PRCS_STS LOAN_STATUS, T.SCHEME_CODE, SUM(DECODE(T.LN_PRCS_STS, 'R', NVL2(T.LOAN_NO, 1, 0), 0)) REPAYING_LOAN_CNT 
                                   FROM (SELECT EI_CDE, LN_PRCS_STS, LOAN_NO, SCHEME_CODE
                                           FROM SFS_REP_STAT_LOAN_REP WHERE SCHEME_CODE = 'TSFS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND FIN_SCHM_ABBR = 'JFS' 
                                         UNION ALL 
                                         SELECT EI_CDE, LN_PRCS_STS, LOAN_NO, SCHEME_CODE
                                           FROM SFS_REP_STAT_LOAN_REP_ADJ WHERE SCHEME_CODE = 'TSFS' AND FIN_SCHM_ABBR = 'JFS') T
                                  GROUP BY T.EI_CDE, T.LN_PRCS_STS, T.SCHEME_CODE) A
                          GROUP BY A.LOAN_STATUS, A.SCHEME_CODE) AL
                      , (SELECT S.LN_PRCS_STS LOAN_STATUS, S.SCHEME_CODE, SUM(S.REPAYING_LOAN_CNT) REPAYING_LOAN_CNT
                           FROM (SELECT T.LN_PRCS_STS, T.EI_CDE, T.SCHEME_CODE, SUM(DECODE(T.LN_PRCS_STS, 'R', 1, 0)) REPAYING_LOAN_CNT
                                   FROM (SELECT LN_PRCS_STS, EI_CDE, SCHEME_CODE
                                           FROM SFS_REP_STAT_FULLY_SETTLED WHERE SCHEME_CODE = 'TSFS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND FIN_SCHM_ABBR = 'JFS'
                                         UNION ALL
                                         SELECT LN_PRCS_STS, EI_CDE, SCHEME_CODE
                                           FROM SFS_REP_STAT_FULLY_SETTLED_ADJ WHERE SCHEME_CODE = 'TSFS' AND FIN_SCHM_ABBR = 'JFS') T			
                                  GROUP BY T.LN_PRCS_STS, T.EI_CDE, T.SCHEME_CODE) S
                          GROUP BY S.LN_PRCS_STS, S.SCHEME_CODE) SL
                  WHERE AL.LOAN_STATUS = SL.LOAN_STATUS (+)
                  GROUP BY AL.SCHEME_CODE) 
               ) RLC
        
        UNION ALL
        
-- REPAYING_LOAN_CNT_Q (- Quarterly)
        SELECT 'REPAYING_LOAN_CNT_Q' AS ITEM
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(RLC_Q.SCHEME_CODE, 'NLSFT', RLC_Q.REPAYING_LOAN_CNT_Q)), '999999999999'))) NLSFT
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(RLC_Q.SCHEME_CODE, 'ENLS', RLC_Q.REPAYING_LOAN_CNT_Q)), '999999999999'))) ENLS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(RLC_Q.SCHEME_CODE, 'NLSPS', RLC_Q.REPAYING_LOAN_CNT_Q)), '999999999999'))) NLSPS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(RLC_Q.SCHEME_CODE, 'TSFS', RLC_Q.REPAYING_LOAN_CNT_Q)), '999999999999'))) TSFS
             , '-' SFAS, '-' JFS, '-' APR
          FROM (
                -- NLSFT
                (SELECT A.SCHEME_CODE, SUM(A.REPAYING_LOAN_CNT_Q - NVL(B.REPAYING_LOAN_CNT_Q, 0)) REPAYING_LOAN_CNT_Q 
                   FROM (SELECT LOAN_STATUS, SCHEME_CODE, SUM(DECODE(LOAN_STATUS||REP_FREQ, 'RQ', 1, 0)) REPAYING_LOAN_CNT_Q FROM NLS_REP_STAT_LOAN_REP 
                          WHERE SCHEME_CODE = 'NLSFT' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) A
                      , (SELECT LOAN_STATUS, SCHEME_CODE, SUM(DECODE(LOAN_STATUS||REP_FREQ, 'RQ', 1, 0)) REPAYING_LOAN_CNT_Q FROM NLS_REP_STAT_FULLY_SETTLED
                          WHERE SCHEME_CODE = 'NLSFT' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) B
                  WHERE A.LOAN_STATUS = B.LOAN_STATUS (+)
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- ENLS
                (SELECT A.SCHEME_CODE, SUM(A.REPAYING_LOAN_CNT_Q - NVL(B.REPAYING_LOAN_CNT_Q, 0)) REPAYING_LOAN_CNT_Q
                   FROM (SELECT T.PRCP_STS LOAN_STATUS, T.SCHEME_CODE, SUM(DECODE(T.PRCP_STS||T.REPAYM_MODE, 'RQ', T.NO_ACCT, 0)) REPAYING_LOAN_CNT_Q   
                           FROM (SELECT PRCP_STS, REPAYM_MODE, SCHEME_CODE, NO_ACCT FROM ENLS_REP_STAT_LOAN_REP
                                  WHERE SCHEME_CODE = 'ENLS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND PRIN_PAID >= 0
                                 UNION ALL
                                 SELECT PRCP_STS, REPAYM_MODE, SCHEME_CODE, NO_ACCT FROM ENLS_REP_STAT_LOAN_REP_ADJ) T
                          GROUP BY T.PRCP_STS, T.SCHEME_CODE) A
                      , (SELECT S.LN_PRCS_STS LOAN_STATUS, SUM(DECODE(S.LN_PRCS_STS||REPAYM_MODE, 'RQ', S.LOAN_CNT, 0)) REPAYING_LOAN_CNT_Q
                           FROM (SELECT EI_CDE, LN_PRCS_STS, REPAYM_MODE, COUNT(LOAN_NO) LOAN_CNT FROM ENLS_REP_STAT_FULLY_SETTLED 
                                  WHERE SCHEME_CODE = 'ENLS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY EI_CDE, LN_PRCS_STS, REPAYM_MODE, SCHEME_CODE) S 
                          GROUP BY S.LN_PRCS_STS) B
                  WHERE A.LOAN_STATUS = B.LOAN_STATUS (+)
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- NLSPS
                (SELECT A.SCHEME_CODE, SUM(A.REPAYING_LOAN_CNT_Q - NVL(B.REPAYING_LOAN_CNT_Q, 0)) REPAYING_LOAN_CNT_Q 
                   FROM (SELECT LOAN_STATUS, SCHEME_CODE, SUM(DECODE(LOAN_STATUS||REP_FREQ, 'RQ', 1, 0)) REPAYING_LOAN_CNT_Q FROM NLS_REP_STAT_LOAN_REP 
                          WHERE SCHEME_CODE = 'NLSPS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) A
                      , (SELECT LOAN_STATUS, SCHEME_CODE, SUM(DECODE(LOAN_STATUS||REP_FREQ, 'RQ', 1, 0)) REPAYING_LOAN_CNT_Q FROM NLS_REP_STAT_FULLY_SETTLED
                          WHERE SCHEME_CODE = 'NLSPS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) B
                  WHERE A.LOAN_STATUS = B.LOAN_STATUS (+)
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- TSFS
                (SELECT AL.SCHEME_CODE, SUM(AL.REPAYING_LOAN_CNT_Q - NVL(SL.REPAYING_LOAN_CNT_Q, 0)) REPAYING_LOAN_CNT_Q
                   FROM (SELECT A.LOAN_STATUS, A.SCHEME_CODE, SUM(A.REPAYING_LOAN_CNT_Q) REPAYING_LOAN_CNT_Q
                           FROM (SELECT T.EI_CDE, T.LN_PRCS_STS LOAN_STATUS, T.SCHEME_CODE, SUM(DECODE(T.LN_PRCS_STS||T.REPAYM_FREQ, 'RQ', 1, 0)) REPAYING_LOAN_CNT_Q
                                   FROM (SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE, REPAYM_FREQ
                                           FROM SFS_REP_STAT_LOAN_REP WHERE SCHEME_CODE = 'TSFS'AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND FIN_SCHM_ABBR = 'TSFS'
                                         UNION ALL
                                         SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE, REPAYM_FREQ
                                           FROM SFS_REP_STAT_LOAN_REP_ADJ WHERE SCHEME_CODE = 'TSFS' AND FIN_SCHM_ABBR = 'TSFS') T
                                  GROUP BY T.EI_CDE, T.LN_PRCS_STS, T.SCHEME_CODE) A
                          GROUP BY A.LOAN_STATUS, A.SCHEME_CODE) AL
                      , (SELECT S.LN_PRCS_STS LOAN_STATUS, S.SCHEME_CODE, SUM(S.REPAYING_LOAN_CNT_Q) REPAYING_LOAN_CNT_Q
                           FROM (SELECT T.LN_PRCS_STS, T.EI_CDE, T.SCHEME_CODE, SUM(DECODE(T.LN_PRCS_STS||T.REPAYM_FREQ, 'RQ', 1, 0)) REPAYING_LOAN_CNT_Q
                                   FROM (SELECT LN_PRCS_STS, EI_CDE, SCHEME_CODE, REPAYM_FREQ
                                           FROM SFS_REP_STAT_FULLY_SETTLED WHERE SCHEME_CODE = 'TSFS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND FIN_SCHM_ABBR = 'TSFS'
                                          UNION ALL
                                         SELECT LN_PRCS_STS, EI_CDE, SCHEME_CODE, REPAYM_FREQ
                                           FROM SFS_REP_STAT_FULLY_SETTLED_ADJ WHERE SCHEME_CODE = 'TSFS'AND FIN_SCHM_ABBR = 'TSFS') T
                                  GROUP BY T.LN_PRCS_STS, T.EI_CDE, T.SCHEME_CODE) S
                          GROUP BY S.LN_PRCS_STS, S.SCHEME_CODE) SL
                  WHERE AL.LOAN_STATUS = SL.LOAN_STATUS (+)
                  GROUP BY AL.SCHEME_CODE)
               ) RLC_Q
        
        UNION ALL
        
-- REPAYING_LOAN_CNT_M (- Monthly)
        SELECT 'REPAYING_LOAN_CNT_M' AS ITEM
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(RLC_M.SCHEME_CODE, 'NLSFT', RLC_M.REPAYING_LOAN_CNT_M)), '999999999999'))) NLSFT
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(RLC_M.SCHEME_CODE, 'ENLS', RLC_M.REPAYING_LOAN_CNT_M)), '999999999999'))) ENLS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(RLC_M.SCHEME_CODE, 'NLSPS', RLC_M.REPAYING_LOAN_CNT_M)), '999999999999'))) NLSPS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(RLC_M.SCHEME_CODE, 'TSFS', RLC_M.REPAYING_LOAN_CNT_M)), '999999999999'))) TSFS
             , '-' SFAS, '-' JFS, '-' APR
          FROM (
                -- NLSFT
                (SELECT A.SCHEME_CODE, SUM(A.REPAYING_LOAN_CNT_M - NVL(B.REPAYING_LOAN_CNT_M, 0)) REPAYING_LOAN_CNT_M 
                   FROM (SELECT LOAN_STATUS, SCHEME_CODE, SUM(DECODE(LOAN_STATUS||REP_FREQ, 'RM', 1, 0)) REPAYING_LOAN_CNT_M FROM NLS_REP_STAT_LOAN_REP 
                          WHERE SCHEME_CODE = 'NLSFT' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) A
                      , (SELECT LOAN_STATUS, SCHEME_CODE, SUM(DECODE(LOAN_STATUS||REP_FREQ, 'RM', 1, 0)) REPAYING_LOAN_CNT_M FROM NLS_REP_STAT_FULLY_SETTLED
                          WHERE SCHEME_CODE = 'NLSFT' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) B
                  WHERE A.LOAN_STATUS = B.LOAN_STATUS (+)
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- ENLS
                (SELECT A.SCHEME_CODE, SUM(A.REPAYING_LOAN_CNT_M - NVL(B.REPAYING_LOAN_CNT_M, 0)) REPAYING_LOAN_CNT_M
                   FROM (SELECT T.PRCP_STS LOAN_STATUS, T.SCHEME_CODE, SUM(DECODE(T.PRCP_STS||T.REPAYM_MODE, 'RM', T.NO_ACCT, 0)) REPAYING_LOAN_CNT_M
                           FROM (SELECT PRCP_STS, REPAYM_MODE, SCHEME_CODE, NO_ACCT FROM ENLS_REP_STAT_LOAN_REP
                                  WHERE SCHEME_CODE = 'ENLS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND PRIN_PAID >= 0
                                 UNION ALL
                                 SELECT PRCP_STS, REPAYM_MODE, SCHEME_CODE, NO_ACCT FROM ENLS_REP_STAT_LOAN_REP_ADJ) T
                          GROUP BY T.PRCP_STS, T.SCHEME_CODE) A
                      , (SELECT S.LN_PRCS_STS LOAN_STATUS, SUM(DECODE(S.LN_PRCS_STS||REPAYM_MODE, 'RM', S.LOAN_CNT, 0)) REPAYING_LOAN_CNT_M
                           FROM (SELECT EI_CDE, LN_PRCS_STS, REPAYM_MODE, COUNT(LOAN_NO) LOAN_CNT FROM ENLS_REP_STAT_FULLY_SETTLED
                                  WHERE SCHEME_CODE = 'ENLS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY EI_CDE, LN_PRCS_STS, REPAYM_MODE, SCHEME_CODE) S
                          GROUP BY S.LN_PRCS_STS) B
                  WHERE A.LOAN_STATUS = B.LOAN_STATUS (+)
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- NLSPS
                (SELECT A.SCHEME_CODE, SUM(A.REPAYING_LOAN_CNT_M - NVL(B.REPAYING_LOAN_CNT_M, 0)) REPAYING_LOAN_CNT_M 
                   FROM (SELECT LOAN_STATUS, SCHEME_CODE, SUM(DECODE(LOAN_STATUS||REP_FREQ, 'RM', 1, 0)) REPAYING_LOAN_CNT_M FROM NLS_REP_STAT_LOAN_REP 
                          WHERE SCHEME_CODE = 'NLSPS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) A
                      , (SELECT LOAN_STATUS, SCHEME_CODE, SUM(DECODE(LOAN_STATUS||REP_FREQ, 'RM', 1, 0)) REPAYING_LOAN_CNT_M FROM NLS_REP_STAT_FULLY_SETTLED
                          WHERE SCHEME_CODE = 'NLSPS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) B
                  WHERE A.LOAN_STATUS = B.LOAN_STATUS (+)
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- TSFS
                (SELECT AL.SCHEME_CODE, SUM(AL.REPAYING_LOAN_CNT_M - NVL(SL.REPAYING_LOAN_CNT_M, 0)) REPAYING_LOAN_CNT_M
                   FROM (SELECT A.LOAN_STATUS, A.SCHEME_CODE, SUM(A.REPAYING_LOAN_CNT_M) REPAYING_LOAN_CNT_M
                           FROM (SELECT T.EI_CDE, T.LN_PRCS_STS LOAN_STATUS, T.SCHEME_CODE, SUM(DECODE(T.LN_PRCS_STS||T.REPAYM_FREQ, 'RM', 1, 0)) REPAYING_LOAN_CNT_M
                                   FROM (SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE, REPAYM_FREQ
                                           FROM SFS_REP_STAT_LOAN_REP WHERE SCHEME_CODE = 'TSFS'AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND FIN_SCHM_ABBR = 'TSFS'
                                         UNION ALL
                                         SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE, REPAYM_FREQ
                                           FROM SFS_REP_STAT_LOAN_REP_ADJ WHERE SCHEME_CODE = 'TSFS' AND FIN_SCHM_ABBR = 'TSFS') T
                                  GROUP BY T.EI_CDE, T.LN_PRCS_STS, T.SCHEME_CODE) A
                          GROUP BY A.LOAN_STATUS, A.SCHEME_CODE) AL
                      , (SELECT S.LN_PRCS_STS LOAN_STATUS, S.SCHEME_CODE, SUM(S.REPAYING_LOAN_CNT_M) REPAYING_LOAN_CNT_M
                           FROM (SELECT T.LN_PRCS_STS, T.EI_CDE, T.SCHEME_CODE, SUM(DECODE(T.LN_PRCS_STS||T.REPAYM_FREQ, 'RM', 1, 0)) REPAYING_LOAN_CNT_M
                                   FROM (SELECT LN_PRCS_STS, EI_CDE, SCHEME_CODE, REPAYM_FREQ
                                           FROM SFS_REP_STAT_FULLY_SETTLED WHERE SCHEME_CODE = 'TSFS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND FIN_SCHM_ABBR = 'TSFS'
                                          UNION ALL
                                         SELECT LN_PRCS_STS, EI_CDE, SCHEME_CODE, REPAYM_FREQ
                                           FROM SFS_REP_STAT_FULLY_SETTLED_ADJ WHERE SCHEME_CODE = 'TSFS' AND FIN_SCHM_ABBR = 'TSFS') T
                                  GROUP BY T.LN_PRCS_STS, T.EI_CDE, T.SCHEME_CODE) S
                          GROUP BY S.LN_PRCS_STS, S.SCHEME_CODE) SL
                  WHERE AL.LOAN_STATUS = SL.LOAN_STATUS (+)
                  GROUP BY AL.SCHEME_CODE)
               ) RLC_M
        
        UNION ALL
        
-- TOTAL_OUTSTANDING_PRINCIPAL (Total Outstanding Principal under Repayment ($M))
        SELECT 'TOTAL_OUTSTANDING_PRINCIPAL' AS ITEM
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(TOP.SCHEME_CODE, 'NLSFT', TOP.TOTAL_OUTSTANDING_PRINCIPAL)), '999,999,999,999.000'))) NLSFT
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(TOP.SCHEME_CODE, 'ENLS', TOP.TOTAL_OUTSTANDING_PRINCIPAL)), '999,999,999,999.000'))) ENLS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(TOP.SCHEME_CODE, 'NLSPS', TOP.TOTAL_OUTSTANDING_PRINCIPAL)), '999,999,999,999.000'))) NLSPS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(TOP.SCHEME_CODE, 'TSFS', TOP.TOTAL_OUTSTANDING_PRINCIPAL)), '999,999,999,999.000'))) TSFS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(TOP.SCHEME_CODE, 'SFAS', TOP.TOTAL_OUTSTANDING_PRINCIPAL)), '999,999,999,999.000'))) SFAS
             , '-' JFS, '-' APR
          FROM (
                -- NLSFT
                (SELECT A.SCHEME_CODE, ROUND(SUM(A.OUTSTANDING_PRINCIPAL) / 1000000, 3) TOTAL_OUTSTANDING_PRINCIPAL
                   FROM (SELECT LOAN_STATUS, SCHEME_CODE, SUM(DECODE(LOAN_STATUS, 'R', UNDEMANDED_PRINCIPAL + PRINCIPAL_UNPAID, 0)) OUTSTANDING_PRINCIPAL FROM NLS_REP_STAT_LOAN_REP 
                          WHERE SCHEME_CODE = 'NLSFT' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) A
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- ENLS
                (SELECT A.SCHEME_CODE, ROUND(SUM(A.OUTSTANDING_PRINCIPAL) / 1000000, 3) TOTAL_OUTSTANDING_PRINCIPAL
                   FROM (SELECT T.PRCP_STS LOAN_STATUS, T.SCHEME_CODE, SUM(DECODE(T.PRCP_STS, 'R', T.UNDER_PRINC + T.PRIN_IN_ARREAR, 0)) OUTSTANDING_PRINCIPAL
                           FROM (SELECT PRCP_STS, REPAYM_MODE, SCHEME_CODE, UNDER_PRINC, PRIN_IN_ARREAR
                                   FROM ENLS_REP_STAT_LOAN_REP
                                  WHERE SCHEME_CODE = 'ENLS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND PRIN_PAID >= 0
                                 UNION ALL
                                 SELECT PRCP_STS, REPAYM_MODE, SCHEME_CODE, UNDER_PRINC, PRIN_IN_ARREAR FROM ENLS_REP_STAT_LOAN_REP_ADJ) T
                                  GROUP BY T.PRCP_STS, T.SCHEME_CODE) A  
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- NLSPS
                (SELECT A.SCHEME_CODE, ROUND(SUM(A.OUTSTANDING_PRINCIPAL) / 1000000, 3) TOTAL_OUTSTANDING_PRINCIPAL
                   FROM (SELECT LOAN_STATUS, SCHEME_CODE, SUM(DECODE(LOAN_STATUS, 'R', UNDEMANDED_PRINCIPAL + PRINCIPAL_UNPAID, 0)) OUTSTANDING_PRINCIPAL FROM NLS_REP_STAT_LOAN_REP 
                          WHERE SCHEME_CODE = 'NLSPS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) A
                  GROUP BY A.SCHEME_CODE)
                 UNION ALL
                -- TSFS
                (SELECT AL.SCHEME_CODE, ROUND(SUM(AL.OUTSTANDING_PRINCIPAL) / 1000000, 3) TOTAL_OUTSTANDING_PRINCIPAL
                   FROM (SELECT A.LOAN_STATUS, A.SCHEME_CODE, SUM(A.OUTSTANDING_PRINCIPAL) OUTSTANDING_PRINCIPAL
                           FROM (SELECT T.EI_CDE, T.LN_PRCS_STS LOAN_STATUS, T.SCHEME_CODE, SUM(DECODE(T.LN_PRCS_STS, 'R', T.UNDEMAND_PRCP + T.ARREAR_PRCP, 0)) OUTSTANDING_PRINCIPAL
                                   FROM (SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE, UNDEMAND_PRCP, ARREAR_PRCP
                                           FROM SFS_REP_STAT_LOAN_REP WHERE SCHEME_CODE = 'TSFS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND FIN_SCHM_ABBR = 'TSFS'
                                         UNION ALL
                                         SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE, UNDEMAND_PRCP, ARREAR_PRCP
                                           FROM SFS_REP_STAT_LOAN_REP_ADJ WHERE SCHEME_CODE = 'TSFS' AND FIN_SCHM_ABBR = 'TSFS') T
                                  GROUP BY T.EI_CDE, T.LN_PRCS_STS, T.SCHEME_CODE) A
                          GROUP BY A.LOAN_STATUS, A.SCHEME_CODE) AL
                  GROUP BY AL.SCHEME_CODE)
                UNION ALL
                -- SFAS
                (SELECT 'SFAS' AS SCHEME_CODE, ROUND(SUM(AL.OUTSTANDING_PRINCIPAL) / 1000000, 3) TOTAL_OUTSTANDING_PRINCIPAL
                   FROM (SELECT A.LOAN_STATUS, A.SCHEME_CODE, SUM(A.OUTSTANDING_PRINCIPAL) OUTSTANDING_PRINCIPAL   
                           FROM (SELECT T.EI_CDE, T.LN_PRCS_STS LOAN_STATUS, T.SCHEME_CODE, SUM(DECODE(T.LN_PRCS_STS, 'R', T.UNDEMAND_PRCP + T.ARREAR_PRCP, 0)) OUTSTANDING_PRINCIPAL
                                   FROM (SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE, UNDEMAND_PRCP, ARREAR_PRCP
                                           FROM SFS_REP_STAT_LOAN_REP WHERE SCHEME_CODE = 'TSFS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND FIN_SCHM_ABBR = 'SFAS'
                                         UNION ALL
                                         SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE, UNDEMAND_PRCP, ARREAR_PRCP   
                                           FROM SFS_REP_STAT_LOAN_REP_ADJ WHERE SCHEME_CODE = 'TSFS' AND FIN_SCHM_ABBR = 'SFAS') T
                                  GROUP BY T.EI_CDE, T.LN_PRCS_STS, T.SCHEME_CODE) A
                          GROUP BY A.LOAN_STATUS, A.SCHEME_CODE) AL
                  GROUP BY AL.SCHEME_CODE)        
               ) TOP
        
        UNION ALL
        
-- TOTAL_OUTSTANDING_PRINCIPAL_Q (- Quarterly)
        SELECT 'TOTAL_OUTSTANDING_PRINCIPAL_Q' AS ITEM
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(TOP_Q.SCHEME_CODE, 'NLSFT', TOP_Q.TOTAL_OUTSTANDING_PRINCIPAL_Q)), '999,999,999,999.000'))) NLSFT
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(TOP_Q.SCHEME_CODE, 'ENLS', TOP_Q.TOTAL_OUTSTANDING_PRINCIPAL_Q)), '999,999,999,999.000'))) ENLS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(TOP_Q.SCHEME_CODE, 'NLSPS', TOP_Q.TOTAL_OUTSTANDING_PRINCIPAL_Q)), '999,999,999,999.000'))) NLSPS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(TOP_Q.SCHEME_CODE, 'TSFS', TOP_Q.TOTAL_OUTSTANDING_PRINCIPAL_Q)), '999,999,999,999.000'))) TSFS
             , '-' SFAS, '-' JFS, '-' APR
          FROM (
                -- NLSFT
                (SELECT A.SCHEME_CODE, ROUND(SUM(A.OUTSTANDING_PRINCIPAL_Q) / 1000000, 3) TOTAL_OUTSTANDING_PRINCIPAL_Q
                   FROM (SELECT LOAN_STATUS, SCHEME_CODE, SUM(DECODE(LOAN_STATUS||REP_FREQ, 'RQ', UNDEMANDED_PRINCIPAL + PRINCIPAL_UNPAID, 0)) OUTSTANDING_PRINCIPAL_Q FROM NLS_REP_STAT_LOAN_REP 
                          WHERE SCHEME_CODE = 'NLSFT' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) A
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- ENLS
                (SELECT A.SCHEME_CODE, ROUND(SUM(A.OUTSTANDING_PRINCIPAL_Q) / 1000000, 3) TOTAL_OUTSTANDING_PRINCIPAL_Q
                   FROM (SELECT T.PRCP_STS LOAN_STATUS, T.SCHEME_CODE, SUM(DECODE(T.PRCP_STS||T.REPAYM_MODE, 'RQ', T.UNDER_PRINC + T.PRIN_IN_ARREAR, 0)) OUTSTANDING_PRINCIPAL_Q
                           FROM (SELECT PRCP_STS, REPAYM_MODE, SCHEME_CODE, UNDER_PRINC, PRIN_IN_ARREAR
                                   FROM ENLS_REP_STAT_LOAN_REP
                                  WHERE SCHEME_CODE = 'ENLS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND PRIN_PAID >= 0
                                 UNION ALL
                                 SELECT PRCP_STS, REPAYM_MODE, SCHEME_CODE, UNDER_PRINC, PRIN_IN_ARREAR FROM ENLS_REP_STAT_LOAN_REP_ADJ) T
                          GROUP BY T.PRCP_STS, T.SCHEME_CODE) A  
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- NLSPS
                (SELECT A.SCHEME_CODE, ROUND(SUM(A.OUTSTANDING_PRINCIPAL_Q) / 1000000, 3) TOTAL_OUTSTANDING_PRINCIPAL_Q
                   FROM (SELECT LOAN_STATUS, SCHEME_CODE, SUM(DECODE(LOAN_STATUS||REP_FREQ, 'RQ', UNDEMANDED_PRINCIPAL + PRINCIPAL_UNPAID, 0)) OUTSTANDING_PRINCIPAL_Q FROM NLS_REP_STAT_LOAN_REP 
                          WHERE SCHEME_CODE = 'NLSPS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) A
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- TSFS
                (SELECT AL.SCHEME_CODE, ROUND(SUM(AL.OUTSTANDING_PRINCIPAL_Q) / 1000000, 3) TOTAL_OUTSTANDING_PRINCIPAL_Q
                   FROM (SELECT A.LOAN_STATUS, A.SCHEME_CODE, SUM(A.OUTSTANDING_PRINCIPAL_Q) OUTSTANDING_PRINCIPAL_Q
                           FROM (SELECT T.EI_CDE, T.LN_PRCS_STS LOAN_STATUS, T.SCHEME_CODE, SUM(DECODE(T.LN_PRCS_STS||REPAYM_FREQ, 'RQ', T.UNDEMAND_PRCP + T.ARREAR_PRCP, 0)) OUTSTANDING_PRINCIPAL_Q
                                   FROM (SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE, UNDEMAND_PRCP, ARREAR_PRCP, REPAYM_FREQ
                                           FROM SFS_REP_STAT_LOAN_REP WHERE SCHEME_CODE = 'TSFS'AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND FIN_SCHM_ABBR = 'TSFS'
                                         UNION ALL
                                         SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE, UNDEMAND_PRCP, ARREAR_PRCP, REPAYM_FREQ
                                           FROM SFS_REP_STAT_LOAN_REP_ADJ WHERE SCHEME_CODE = 'TSFS' AND FIN_SCHM_ABBR = 'TSFS') T
                                  GROUP BY T.EI_CDE, T.LN_PRCS_STS, T.SCHEME_CODE) A
                          GROUP BY A.LOAN_STATUS, A.SCHEME_CODE) AL
                  GROUP BY AL.SCHEME_CODE)
               ) TOP_Q
        
        UNION ALL
        
-- TOTAL_OUTSTANDING_PRINCIPAL_M (- Monthly)
        SELECT 'TOTAL_OUTSTANDING_PRINCIPAL_M' AS ITEM
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(TOP_M.SCHEME_CODE, 'NLSFT', TOP_M.TOTAL_OUTSTANDING_PRINCIPAL_M)), '999,999,999,999.000'))) NLSFT
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(TOP_M.SCHEME_CODE, 'ENLS', TOP_M.TOTAL_OUTSTANDING_PRINCIPAL_M)), '999,999,999,999.000'))) ENLS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(TOP_M.SCHEME_CODE, 'NLSPS', TOP_M.TOTAL_OUTSTANDING_PRINCIPAL_M)), '999,999,999,999.000'))) NLSPS
             , RTRIM(LTRIM(TO_CHAR(SUM(DECODE(TOP_M.SCHEME_CODE, 'TSFS', TOP_M.TOTAL_OUTSTANDING_PRINCIPAL_M)), '999,999,999,999.000'))) TSFS
             , '-' SFAS, '-' JFS, '-' APR
          FROM (
                -- NLSFT
                (SELECT A.SCHEME_CODE, ROUND(SUM(A.OUTSTANDING_PRINCIPAL_M) / 1000000, 3) TOTAL_OUTSTANDING_PRINCIPAL_M
                   FROM (SELECT LOAN_STATUS, SCHEME_CODE, SUM(DECODE(LOAN_STATUS||REP_FREQ, 'RM', UNDEMANDED_PRINCIPAL + PRINCIPAL_UNPAID, 0)) OUTSTANDING_PRINCIPAL_M FROM NLS_REP_STAT_LOAN_REP 
                          WHERE SCHEME_CODE = 'NLSFT' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) A
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- ENLS
                (SELECT A.SCHEME_CODE, ROUND(SUM(A.OUTSTANDING_PRINCIPAL_M) / 1000000, 3) TOTAL_OUTSTANDING_PRINCIPAL_M
                   FROM (SELECT T.PRCP_STS LOAN_STATUS, T.SCHEME_CODE, SUM(DECODE(T.PRCP_STS||T.REPAYM_MODE, 'RM', T.UNDER_PRINC + T.PRIN_IN_ARREAR, 0)) OUTSTANDING_PRINCIPAL_M
                           FROM (SELECT PRCP_STS, REPAYM_MODE, SCHEME_CODE, UNDER_PRINC, PRIN_IN_ARREAR
                                   FROM ENLS_REP_STAT_LOAN_REP
                                  WHERE SCHEME_CODE = 'ENLS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND PRIN_PAID >= 0
                                 UNION ALL
                                 SELECT PRCP_STS, REPAYM_MODE, SCHEME_CODE, UNDER_PRINC, PRIN_IN_ARREAR FROM ENLS_REP_STAT_LOAN_REP_ADJ) T
                          GROUP BY T.PRCP_STS, T.SCHEME_CODE) A  
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- NLSPS
                (SELECT A.SCHEME_CODE, ROUND(SUM(A.OUTSTANDING_PRINCIPAL_M) / 1000000, 3) TOTAL_OUTSTANDING_PRINCIPAL_M
                   FROM (SELECT LOAN_STATUS, SCHEME_CODE, SUM(DECODE(LOAN_STATUS||REP_FREQ, 'RM', UNDEMANDED_PRINCIPAL + PRINCIPAL_UNPAID, 0)) OUTSTANDING_PRINCIPAL_M FROM NLS_REP_STAT_LOAN_REP 
                          WHERE SCHEME_CODE = 'NLSPS' AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') GROUP BY LOAN_STATUS, SCHEME_CODE) A
                  GROUP BY A.SCHEME_CODE)
                UNION ALL
                -- TSFS
                (SELECT AL.SCHEME_CODE, ROUND(SUM(AL.OUTSTANDING_PRINCIPAL_M) / 1000000, 3) TOTAL_OUTSTANDING_PRINCIPAL_M
                   FROM (SELECT A.LOAN_STATUS, A.SCHEME_CODE, SUM(A.OUTSTANDING_PRINCIPAL_M) OUTSTANDING_PRINCIPAL_M
                           FROM (SELECT T.EI_CDE, T.LN_PRCS_STS LOAN_STATUS, T.SCHEME_CODE, SUM(DECODE(T.LN_PRCS_STS||REPAYM_FREQ, 'RM', T.UNDEMAND_PRCP + T.ARREAR_PRCP, 0)) OUTSTANDING_PRINCIPAL_M
                                   FROM (SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE, UNDEMAND_PRCP, ARREAR_PRCP, REPAYM_FREQ
                                           FROM SFS_REP_STAT_LOAN_REP WHERE SCHEME_CODE = 'TSFS'AND POSITION_DATE = TO_DATE($P{P_CUT_OFF_DATE}, 'YYYYMMDD') AND FIN_SCHM_ABBR = 'TSFS'
                                         UNION ALL
                                         SELECT EI_CDE, LN_PRCS_STS, SCHEME_CODE, UNDEMAND_PRCP, ARREAR_PRCP, REPAYM_FREQ
                                           FROM SFS_REP_STAT_LOAN_REP_ADJ WHERE SCHEME_CODE = 'TSFS' AND FIN_SCHM_ABBR = 'TSFS') T
                                  GROUP BY T.EI_CDE, T.LN_PRCS_STS, T.SCHEME_CODE) A
                          GROUP BY A.LOAN_STATUS, A.SCHEME_CODE) AL
                  GROUP BY AL.SCHEME_CODE)
               ) TOP_M
        
        UNION ALL 

-- REPAYING_LOAN_PRINCIPAL_CUMULATIVE (Repayment of loan principal (cumulative) ($M)) 
        SELECT 'REPAYING_LOAN_PRINCIPAL_CUMULATIVE' AS ITEM, '-' NLSFT, '-' ENLS, '-' NLSPS
             , '-' TSFS, '-' SFAS, '-' JFS
             , RTRIM(LTRIM(TO_CHAR(SUM(ROUND(ADJUSTED_PRINCIPAL_REPAID / 1000000, 2)), '999,999,999,999.00'))) APR 
          FROM REP_STAT_LOAN_REP_SUMMARY
         WHERE SCHEME_TYPE = 'NLS'
           AND FINANCIAL_YEAR <= (CASE 
                                    WHEN TRUNC($P{P_CUT_OFF_DATE}) BETWEEN SUBSTR($P{P_CUT_OFF_DATE}, 1, 4) || '0401' AND SUBSTR($P{P_CUT_OFF_DATE}, 1, 4) || '1231' THEN 
                                      SUBSTR($P{P_CUT_OFF_DATE}, 1, 4) || '-' || (SUBSTR($P{P_CUT_OFF_DATE}, 1, 4) + 1)
                                    ELSE 
                                      (SUBSTR($P{P_CUT_OFF_DATE}, 1, 4) - 1) || '-' || SUBSTR($P{P_CUT_OFF_DATE}, 1, 4)
                                  END)
)]]>
	</queryString>
